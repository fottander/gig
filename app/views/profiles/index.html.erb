<h3>Browse all freelancers</h3>
<% @profiles.each do |profile| %>
<div class="card profile-card-action-icons">
    <div class="card-section">
        <div class="profile-card-header">
            <div class="profile-card-avatar">
                <img class="avatar-image" src="https://pbs.twimg.com/profile_images/732634911761260544/OxHbNdTF.jpg" alt="Harry Manchanda">
            </div>
            <div class="profile-card-author">
                <h4 class="author-title"><%= link_to profile.username, profile_path(profile)%></h4>
                Title: <%= profile.title %>
                <div class="small-12 medium-2 columns add-friend">
                  <%= link_to 'View', profile_path(profile), class: 'small button inc' %>
                  <%= link_to 'Invite', profile_path(profile), class: 'small button alert' %>
                </div>
            </div>
        </div>
        <div class="profile-card-about">
            Beskrivning:
            <%= profile.description %>
            <br>
            | Kategorier:
            <%= profile.category %>
            | Budget:
            <%= profile.rate %>
            | Skills:
            <%= profile.skill %>
            | Stad:
            <%= profile.city %>
            |<br>
        </div>
    </div>
</div>
<% end %>
